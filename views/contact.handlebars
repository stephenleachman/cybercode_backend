        <div class=" col-12 col-md-7 col-lg-6 col-xl-5 form-body shadow">
        <div class="card-body">
          <form id="contactForm" action="email_sent" method="POST">
            <h4 class="text-success text-center">{{msg}}</h4>
            <div class="form-group">
              <label for="inputName">Name <span class="text-info">*</span></label>
              <input name="name" type="name" class="form-control" id="inputName" aria-describedby="emailHelp" placeholder="Enter Name" required>
            </div>
            <div class="form-group">
              <label for="inputEmail">Email <span class="text-info">*</span></label>
              <input name="email" type="email" class="form-control" id="inputEmail" placeholder="Enter Email" required>
            </div>
            <div class="form-group">
              <label for="inputSubject">Subject</label>
              <select name="subject" class="form-control" id="inputSubject">
                <option>I need a website</option>
                <option>I need some graphics design work done</option>
                <option>I need a web app</option>
                <option>I have an investment related question</option>
                <option>Just to chat</option>
              </select>
              </div>
              <div class="form-group">
                <label for="inputMessage">Message <span class="text-info">*</span></label>
                <textarea name="message" class="form-control" id="inputMessage" rows="3" required></textarea>
              </div>
              <input type="hidden" name="_next" value="contact.html" />
              
              <button id="submit" type="submit" class="btn float-right btn-info mb-3 text-light">Submit</button>
            </form>
          </div>
        </div>
